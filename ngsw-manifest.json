{
  "external":{
    "urls":[

      {"url":"https://fonts.googleapis.com/icon?family=Material+Icons"}

    ]
  },
  "dynamic":{
    "group":[
      {
          "name": "offline",
          "urls":{
            "https://collect.qcitech.org": {
                "match": "prefix"
            }
          },
          "cache":{
            "optimizeFor": "freshness",
            "maxAgeMs":60000
          }
        }
      ]
  },
  "routing": {
    "index": "/index.html",
    "routes" :{
      "/": {
        "prefix": false
      },
      "/formListing": {
        "prefix": true
      },
      "/form": {
        "prefix": true
      },
      "/login": {
        "prefix": true
      },
      "/help": {
        "prefix": true
      }
    }
  }
}
